generator client {
  provider = "prisma-client-js"
  output   = "../../prismaClient"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../app.db"
}

model SequelizeMeta {
  name String @id
}

model tasks {
  actual   Int?
  estimate Int?
  id       Int    @default(autoincrement()) @id
  name     String
  status   Int
}