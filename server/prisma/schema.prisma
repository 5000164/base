generator client {
  provider = "prisma-client-js"
  output   = "../../prismaClient"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../app.db"
}

model SequelizeMeta {
  name String @id
}

model tasks {
  id                Int    @id @default(autoincrement())
  name              String
  status            Int
  estimate          Int?
  actual            Int?
  status_changed_at Int?
}

model templates {
  id                Int              @id @default(autoincrement())
  name              String
  status            Int
  status_changed_at Int?
  template_tasks    template_tasks[]
}

model template_tasks {
  id         Int        @id @default(autoincrement())
  templateId Int?
  name       String
  estimate   Int?
  templates  templates? @relation(fields: [templateId], references: [id])
}
