generator client {
  provider = "prisma-client-js"
  output   = "../../prismaClient"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../app.db"
}

model SequelizeMeta {
  name String @id
}

model tasks {
  actual            Int?
  estimate          Int?
  id                Int    @default(autoincrement()) @id
  name              String
  status            Int
  status_changed_at Int?
}

model templates {
  id                Int              @default(autoincrement()) @id
  name              String
  status            Int
  status_changed_at Int?
  template_tasks    template_tasks[]
}

model template_tasks {
  estimate   Int?
  id         Int        @default(autoincrement()) @id
  name       String
  templateId Int?
  templates  templates? @relation(fields: [templateId], references: [id])
}