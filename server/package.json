{
  "name": "server",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "ts-node-dev --no-notify src/server.ts",
    "prebuild": "rm -rf ../dist/server && mkdir -p ../dist/server",
    "build": "tsc",
    "postbuild": "cp src/generated/schema/schema.graphql ../dist/server/generated/schema/schema.graphql && cp -R src/migrations ../dist/server/migrations",
    "prisma:format": "npx prisma format",
    "prisma:introspect": "npx prisma introspect",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "cors": "^2.8.5",
    "electron-log": "^4.3.4",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "prisma": "^2.20.1",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "umzug": "3.0.0-beta.9"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/validator": "^13.1.3",
    "@types/ws": "^7.4.1",
    "prettier": "2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "4.2.4"
  }
}
