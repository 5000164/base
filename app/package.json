{
  "name": "base",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Base",
    "files": [
      "index.js",
      {
        "from": "../server/build",
        "to": "./server",
        "filter": [
          "!prismaClient"
        ]
      },
      {
        "from": "../client/build",
        "to": "./client"
      }
    ],
    "extraResources": [
      {
        "from": "../server/build/prismaClient",
        "to": "./prismaClient"
      }
    ]
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.3",
    "electron-log": "^4.1.2",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^5.0.0",
    "graphql-yoga": "^1.18.3",
    "sequelize": "github:sequelize/sequelize#e02594fb5b8b18fe68a77a0a0f51620a9d96acb9",
    "sqlite3": "^4.2.0",
    "umzug": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "electron": "8.2.1",
    "electron-builder": "^22.4.1",
    "prettier": "2.0.4"
  }
}
