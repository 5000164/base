{
  "name": "Base",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Base",
    "files": [
      "index.js",
      {
        "from": "../dist/server",
        "to": "./server"
      },
      {
        "from": "../dist/client",
        "to": "./client"
      }
    ],
    "extraResources": [
      {
        "from": "../prismaClient",
        "to": "./prismaClient"
      }
    ]
  },
  "dependencies": {
    "@prisma/client": "^2.17.0",
    "cors": "^2.8.5",
    "electron-log": "^4.3.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "prisma": "^2.17.0",
    "sequelize": "^6.5.0",
    "sqlite3": "^5.0.2",
    "umzug": "3.0.0-beta.9"
  },
  "devDependencies": {
    "electron": "11.2.3",
    "electron-builder": "^22.9.1",
    "prettier": "2.2.1"
  }
}
