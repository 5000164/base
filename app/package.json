{
  "name": "Base",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Base",
    "files": [
      "index.js",
      {
        "from": "../dist/server",
        "to": "./server"
      },
      {
        "from": "../dist/client",
        "to": "./client"
      }
    ],
    "extraResources": [
      {
        "from": "../prismaClient",
        "to": "./prismaClient"
      }
    ]
  },
  "dependencies": {
    "@prisma/client": "^2.26.0",
    "cors": "^2.8.5",
    "electron-log": "^4.3.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "prisma": "^2.26.0",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.3",
    "umzug": "3.0.0-beta.9"
  },
  "devDependencies": {
    "electron": "13.1.6",
    "electron-builder": "^22.11.7",
    "prettier": "2.3.2"
  }
}
