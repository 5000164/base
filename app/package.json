{
  "name": "base",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Base",
    "files": [
      "index.js",
      {
        "from": "../server/build",
        "to": "./server",
        "filter": [
          "!prismaClient"
        ]
      },
      {
        "from": "../client/build",
        "to": "./client"
      }
    ],
    "extraResources": [
      {
        "from": "../server/build/prismaClient",
        "to": "./prismaClient"
      }
    ]
  },
  "dependencies": {
    "@prisma/client": "^2.13.1",
    "electron-log": "^4.3.1",
    "graphql-import": "^1.0.2",
    "graphql-tools": "5.0.0",
    "graphql-yoga": "^1.18.3",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "umzug": "3.0.0-beta.9"
  },
  "devDependencies": {
    "electron": "11.1.1",
    "electron-builder": "^22.9.1",
    "prettier": "2.2.1"
  }
}
